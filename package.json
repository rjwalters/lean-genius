{
  "name": "lean-genius",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm annotations:build && pnpm research:build && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "pnpm build && wrangler pages deploy dist --project-name=lean-genius --commit-dirty=true",
    "annotations:build": "tsx scripts/annotations/build.ts",
    "annotations:validate": "tsx scripts/annotations/build.ts --strict",
    "annotations:migrate": "tsx scripts/annotations/resolver.ts migrate",
    "annotations:parse": "tsx scripts/annotations/resolver.ts parse",
    "research:sync": "tsx scripts/research/sync-data.ts",
    "research:build": "pnpm research:sync && tsx scripts/research/build.ts",
    "db:export": "tsx scripts/research/db-export.ts",
    "db:rebuild": "tsx scripts/research/db-rebuild.ts",
    "aristotle": "./research/scripts/aristotle-status.sh",
    "aristotle:retrieve": "./research/scripts/aristotle-status.sh --retrieve",
    "aristotle:json": "./research/scripts/aristotle-status.sh --json"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "katex": "^0.16.27",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.11.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.4.0",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251221.0",
    "@eslint/js": "^9.39.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/katex": "^0.16.7",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-katex": "^3.0.4",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
