continuity, we get the classic statement\ntheorem ftc_part1_continuous {f : ‚Ñù ‚Üí ‚Ñù} {a : ‚Ñù}\n    (hf : Continuous f) (x : ‚Ñù) :\n    HasDerivAt (integralFunction f a) (f x) x := ","_type":"text"},{"messages":[],"goals":[{"name":"","hypotheses":[],"conclusion":"Goals accomplished! üêô","_type":"goal"}],"contents":[{"typeinfo":null,"semanticType":null,"raw":"by","link":null,"docstring":null,"_type":"token"}],"_type":"sentence"},{"messages":[],"goals":[{"name":"","hypotheses":[{"type":"‚Ñù ‚Üí ‚Ñù","names":["f"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["a"],"body":"","_type":"hypothesis"},{"type":"Continuous f","names":["hf"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["x"],"body":"","_type":"hypothesis"}],"conclusion":"HasDerivAt (integralFunction f a) (f x) x","_type":"goal"}],"contents":[{"typeinfo":null,"semanticType":null,"raw":"\n  ","link":null,"docstring":null,"_type":"token"}],"_type":"sentence"},{"messages":[],"goals":[{"name":"hf_cont","hypotheses":[{"type":"‚Ñù ‚Üí ‚Ñù","names":["f"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["a"],"body":"","_type":"hypothesis"},{"type":"Continuous f","names":["hf"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["x"],"body":"","_type":"hypothesis"}],"conclusion":"ContinuousAt f x","_type":"goal"},{"name":"hf_int","hypotheses":[{"type":"‚Ñù ‚Üí ‚Ñù","names":["f"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["a"],"body":"","_type":"hypothesis"},{"type":"Continuous f","names":["hf"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["x"],"body":"","_type":"hypothesis"}],"conclusion":"IntervalIntegrable f volume a x","_type":"goal"},{"name":"hf_meas","hypotheses":[{"type":"‚Ñù ‚Üí ‚Ñù","names":["f"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["a"],"body":"","_type":"hypothesis"},{"type":"Continuous f","names":["hf"],"body":"","_type":"hypothesis"},{"type":"‚Ñù","names":["x"],"body":"","_type":"hypothesis"}],"conclusion":"StronglyMeasurableAtFilter f 